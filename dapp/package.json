{
    "name": "dapp",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "vercel-build": "npx prisma generate --data-proxy && next build",
        "codegen": "graphql-codegen --config codegen.ts",
        "watch": "graphql-codegen -w",
        "prisma": "dotenv cross-env DATABASE_URL=$MIGRATE_DATABASE_URL prisma",
        "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
        "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
        "component": "cypress open --component",
        "component:headless": "cypress run --component"
    },
    "dependencies": {
        "@apollo/client": "^3.7.7",
        "@next/font": "13.1.1",
        "@pothos/core": "^3.25.0",
        "@pothos/plugin-prisma": "^3.41.0",
        "@prisma/client": "^4.9.0",
        "@types/node": "18.11.18",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "axios": "^1.2.3",
        "cypress": "cypress open",
        "dateformat": "^5.0.3",
        "eslint": "8.31.0",
        "eslint-config-next": "13.1.1",
        "ethers": "^5.7.2",
        "graphql": "^16.6.0",
        "graphql-yoga": "^3.5.1",
        "ipfs-http-client": "^60.0.0",
        "next": "13.1.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "swr": "^2.0.3",
        "wagmi": "^0.11.5"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "3.0.0",
        "@graphql-codegen/client-preset": "2.0.0",
        "@types/dateformat": "^5.0.0",
        "cross-env": "^7.0.3",
        "cypress": "^12.5.1",
        "dotenv-cli": "^7.0.0",
        "node-global-proxy": "^1.0.1",
        "prisma": "^4.9.0",
        "start-server-and-test": "^1.15.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    }
}
